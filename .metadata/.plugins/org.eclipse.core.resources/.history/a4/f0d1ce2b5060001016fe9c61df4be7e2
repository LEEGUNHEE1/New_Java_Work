package com.day16;

//Generic

class Box<T>{
	
	private T t;
	
	public void set(T t) {
		this.t = t;
	}
	
	public T get() {
		return t;
	}
	
}

public class Test2 {

	public static void main(String[] args) {
		
		Box b1 = new Box();
		b1.set(30);//upcast
		Integer i = (Integer)b1.get();//downcast
		System.out.println(i);
		
		
		Box<Integer> b2 = new Box<Integer>();
		b2.set(10);
		
		
		

	}

}






















