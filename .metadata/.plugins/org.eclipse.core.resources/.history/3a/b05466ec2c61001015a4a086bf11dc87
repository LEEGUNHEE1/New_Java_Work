package com.day17;

import java.io.FileInputStream;

//FileInputStream
//FileOutputStream
public class Test8 {

	public static void main(String[] args) {

		try {
			
			FileInputStream fis = 
					new FileInputStream("c:\\doc\\test.txt");
			
			int data;
			while((data=fis.read())!=-1) {
				System.out.println((char)data);
			}
			
			
			
		} catch (Exception e) {
			
		}
		
		
		
	}

}
