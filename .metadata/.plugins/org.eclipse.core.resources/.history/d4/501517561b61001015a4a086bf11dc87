package com.day17;
//데몬스레드
//다른스레드에 도움을 주는 스레드로 
//메인스레드가 종료가 되면 프로세스가 종료 된다

class MyThread2 implements Runnable{

	@Override
	public void run() {
		
		for(int i=1;i<=20;i++) {
			System.out.println(i);
			
			try {
				Thread.sleep(1000);
			} catch (Exception e) {
				
			}
			
		}
		
	}
	
}

public class Test2 {

	public static void main(String[] args) {

	}

}
